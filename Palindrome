import java.io.*;
import java.util.*;
class palindrome
{
public static void main(String args[])
{
int n,s,value=0,r;
Scanner j=new Scanner(System.in);
System.out.println("Enter the number");
n=j.nextInt();
s=n;
while(n>0)
{
r=n%10;
value=(value*10)+r;
n=n/10;
}
if(s==value)
System.out.println("it is palindrome");
else
System.out.println("It is not a palindrome");
}
}
